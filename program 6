#include<iostream>
using namespace std;
int main()
{

	cout<<"\nCharcter Data Type Size ::"<<sizeof(char)<<" Byte";
	cout<<"\nInteger Data Type Size  ::"<<sizeof(int)<<" Byte";
	cout<<"\nFloat Data Type Size    ::"<<sizeof(float)<<" Byte";
	cout<<"\nDouble Data Type Size   ::"<<sizeof(double)<<" Byte";
	
	return 0;
}
